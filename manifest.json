{
    "name": "incognito door",
    "description": "add incognito open button",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["tabs", "activeTab", "scripting", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }